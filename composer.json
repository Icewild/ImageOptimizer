{
  "name": "Icewild/image-optimizer",
  "description": "Simple PHP Image Optimizer",
  "keywords": ["image optimize", "image optimizer"],
  "homepage": "https://github.com/",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "php-mock/php-mock-phpunit": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Grachev",
      "email": "me@7ag.ru"
    }
  ],
  "minimum-stability": "beta",
  "autoload": {
    "psr-4": {"Icewild\\": "src/"}
  },
  "scripts": {
    "test": "php -c ./php.ini ./vendor/bin/phpunit",
    "coverage": "php -c ./php.ini ./vendor/bin/phpunit  --whitelist src/ --coverage-html coverage"
  }
}
